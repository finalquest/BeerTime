{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": ["error", { "ignore": ["navigation"] }],
    "global-require":"off",
  },
  "globals": {
    "shallowRenderProp": true,
    "shallowHOC": true
  }
}
